import "primitives/core.futil";
import "primitives/memories/comb.futil";

component main() -> () {
  cells {
    @external(1) a = comb_mem_d1(32, 1, 1);
  }

  wires {
    group top {
      a.addr0 = 1'b0;
      a.write_en = 1'b1;
      a.write_data = 32'd1;
      top[done] = a.done;
    }

    group middle {
      top[go] = 1'b1;
      middle[done] = top[done];
    }

    group bottom {
    middle[go] = 1'b1;
    bottom[done] = middle[done];
    }
  }

  control {
      bottom;
  }
}
