// -p validate -p static-promotion
import "primitives/core.futil";
import "primitives/binary_operators.futil";
component main(@go go: 1, @clk clk: 1, @reset reset: 1) -> (@done done: 1) {
  cells {
    mult = std_mult_pipe(32);
    add = std_add(32);
    tmp = std_reg(32);
  }
  wires {
    comb group comb_invoke0 {
      add.left = 32'd2;
      add.right = mult.out;
    }
  }
  control {
    seq {
      invoke mult(
        left = 32'd2,
        right = 32'd3
      )();
      invoke tmp(
        in = add.out
      )() with comb_invoke0;
    }
  }
}
